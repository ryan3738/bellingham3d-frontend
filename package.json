{
  "name": "bellingham-3d-frontend",
  "author": "Ryan Fraser",
  "version": "0.3.0",
  "description": "Bellingham 3D e-commerce website! Tech used includes: React, GraphQL, Next, and Apollo",
  "keywords": [
    "website",
    "nextjs",
    "ecommerce"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "next -p 7777",
    "build": "next build",
    "start": "next start -p 7777",
    "test": "jest --watch NODE_ENV=test",
    "apollo:generate": "apollo codegen:generate --target typescript --excludes=node_modules/* --includes=[**/*.tsx,**/*.ts] --tagName=gql --outputFlat types/generated --passthroughCustomScalars --customScalarsPrefix Db",
    "type": "tsc --watch"
  },
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@apollo/link-error": "^2.0.0-beta.3",
    "@apollo/react-ssr": "^4.0.0",
    "@stripe/react-stripe-js": "^1.5.0",
    "@stripe/stripe-js": "^1.18.0",
    "apollo": "^2.33.6",
    "apollo-upload-client": "^16.0.0",
    "babel-core": "^6.26.3",
    "babel-plugin-styled-components": "^1.13.2",
    "date-fns": "^2.24.0",
    "downshift": "^6.1.7",
    "graphql": "^15.6.0",
    "graphql-tag": "^2.12.5",
    "graphql-upload": "^12.0.0",
    "lodash.debounce": "^4.0.8",
    "next": "^11.1.2",
    "next-with-apollo": "^5.2.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-transition-group": "^4.4.2",
    "styled-components": "^5.3.1",
    "uuid": "^8.3.2",
    "waait": "^1.0.5"
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/graphql": "^14.5.0",
    "@types/node": "^16.10.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.24",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.2",
    "casual": "^1.6.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.2",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.4.3"
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
